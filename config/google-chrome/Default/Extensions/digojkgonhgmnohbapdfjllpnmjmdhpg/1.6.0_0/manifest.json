{
   "author": "Procwise BV",
   "background": {
      "persistent": false,
      "scripts": [ "background-script.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content-script.js" ],
      "matches": [ "https://*.proctorexam.com/*" ]
   } ],
   "description": "Lets you share your screen when taking an online exam on ProctorExam - https://www.proctorexam.com",
   "homepage_url": "https://proctorexam.com/",
   "icons": {
      "128": "logo128.png",
      "16": "logo16.png",
      "48": "logo48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "34",
   "name": "ProctorExam Screen Sharing",
   "permissions": [ "desktopCapture", "activeTab", "tabs", "https://*.proctorexam.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.6.0",
   "web_accessible_resources": [ "icon.png" ]
}
